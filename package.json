  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "crx:unsigned-zip": "crx pack build --zip-output app.zip",
    "crx:compress": "crx pack build -p key.pem -o app.crx",
    "crx:compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:fix": "eslint --ignore-path .eslintignore --fix .",
    "format:base": "prettier --config './.prettierrc.yaml' --ignore-path './.eslintignore' --write",
    "format": "npm run format:base './**/*.{js,jsx,json}'",
    "format-fix": "npm run format && npm run lint:fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run format:base",
      "eslint --ignore-path .eslintignore --fix",
      "git add"
    ]
  },
